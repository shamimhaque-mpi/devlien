    "./string": "./framework/App/Helpers/String.js",
    "./dir": "./framework/App/Helpers/DIR.js",
    "./config": "./framework/App/Helpers/Config.js",
    "./system": "./framework/App/Helpers/System.js",
    "./middleware/auth": "./utilities/middlewares/Auth.js",
    "./view": "./framework/App/ViewEngine/View.js",
    "./accessToken": "./framework/App/Authentication/AccessToken.js",
