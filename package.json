{
  "name": "devlien",
  "version": "1.0.4",
  "description": "Devlien is a lightweight, zero-dependency Node.js framework with clean MVC structure, built-in ORM, and intuitive routing for rapid backend development.",
  "exports": {
    ".":"./index.js",
    "./run":"./framework/Run.js",
    "./listener":"./bin/server/Listener.js",
    "./route":"./framework/App/Route/Route.js",
    "./model":"./framework/App/Eloquent/Model.js",
    "./model/authenticatable": "./framework/App/Eloquent/Authenticatable.js",
    "./model/personalAccessToken":  "./utilities/models/PersonalAccessToken.js",
    "./database":"./framework/App/Database/Mysql.js",
    "./db":"./framework/App/Database/DB.js",
    "./migration":"./framework/App/Database/Migration.js",
    "./controller": "./framework/App/Http/Controller/BaseController.js",
    "./resouceCollection": "./framework/App/Http/Resource/ResouceCollection.js",
